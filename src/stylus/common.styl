
body
  margin: 0px
  padding: 0
  min-height: 100%
  color: #454545
  background-color: #f2f2f2

*
  margin: 0px

.pull-right
  float: right !important

.c_960
  width: 960px
  margin-left: auto
  margin-right: auto

.header
  background-color: #583330

  .header-content
    width: 960px
    margin: 0 auto
    line-height: 60px
    color: #fff
    font-size: 18px

    .nav-in-header
      a
        cursor: pointer
        color: #fff
        font-size:14px

.copyright
  position: absolute
  bottom:0
  left:0
  right:0
  text-align:center
  margin-top:50px
  border-top:solid 1px #dedede
  background-color:#F5F5F5
  line-height:30px
  min-height:60px

p
  -webkit-margin-before: 1em
  -webkit-margin-after: 1em
  -webkit-margin-start: 0px
  -webkit-margin-end: 0px
